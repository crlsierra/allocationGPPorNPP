	journal = {Journal of Ecology},
	keywords = {carbon use efficiency, ecosystem respiration, global carbon cycle, model data assimilation, transit times, tropical forests},
	number = {8},
	pages = {2845-2855},
	title = {The fate and transit time of carbon in a tropical forest},
	url = {https://besjournals.onlinelibrary.wiley.com/doi/abs/10.1111/1365-2745.13723},
	volume = {109},
	year = {2021},
	bdsk-url-1 = {https://besjournals.onlinelibrary.wiley.com/doi/abs/10.1111/1365-2745.13723},
