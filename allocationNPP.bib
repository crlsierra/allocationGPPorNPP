	issn = {0829-318X},
	journal = {Tree Physiology},
	month = {mar},
	number = {2},
	pages = {119--135},
	title = {{A model analysis of the influence of root and foliage allocation on forest production and competition between trees}},
	url = {http://treephys.oxfordjournals.org/content/12/2/119.short},
	volume = {12},
	year = {1993},
	bdsk-url-1 = {http://treephys.oxfordjournals.org/content/12/2/119.short},
